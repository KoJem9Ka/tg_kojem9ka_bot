generator client {
  provider = "prisma-client-js"
  output   = "client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model memory {
  id   Int     @id @default(autoincrement())
  date String? @db.VarChar(100)
}

model signed_chats {
  id String @id @db.VarChar(100)
}
